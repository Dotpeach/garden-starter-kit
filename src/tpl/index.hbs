          </th>
        {{/if}}
      </tr>
    </thead>
    <tbody>
    {{#compose src="pages/**/*.hbs"}}
      <tr>
        <td><a href="pages/{{@basename}}.html">{{@title}}</a><br><small>{{@description}}</small></td>
        {{#if INDEX.HTML}}<td>{{@META.HTML}}</td>{{/if}}
