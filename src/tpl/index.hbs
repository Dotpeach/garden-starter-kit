---
layout: documentation.hbs

title: <%- pkg.title %>
description: <%- pkg.description %>
logo : <%- pkg.logo %>
---


<header>
  <div class="title">
    {{#if logo}}<img alt="" src="{{logo}}">{{/if}}
    <h1>{{title}}</h1>
  </div>
  {{#if description}}<p>{{description}}</p>{{/if}}
  <p>Accéder à la <a href="/docs">documentation</a></p>
</header>

<section>
  <h2>Liste des pages</h2>

  <div class="table">
  <table>
    <thead>
      <tr>
        <th scope="col" rowspan="2">Page</th>
        {{#if INDEX.HTML}}
          <th scope="col" rowspan="2">
            {{#if INDEX.HTML.url}}<a href="{{INDEX.HTML.url}}">{{/if}}{{INDEX.HTML.label}}{{#if INDEX.HTML.url}}</a>{{/if}}
          </th>
        {{/if}}
        {{#if INDEX.CSS}}
          <th scope="col" rowspan="2">
            {{#if INDEX.CSS.url}}<a href="{{INDEX.CSS.url}}">{{/if}}{{INDEX.CSS.label}}{{#if INDEX.CSS.url}}</a>{{/if}}
          </th>
        {{/if}}
        {{#if INDEX.JS}}
          <th scope="col" rowspan="2">
            {{#if INDEX.JS.url}}<a href="{{INDEX.JS.url}}">{{/if}}{{INDEX.JS.label}}{{#if INDEX.JS.url}}</a>{{/if}}
          </th>
        {{/if}}
        {{#if INDEX.GROUP1.length}}
        <th scope="col" colspan="{{INDEX.GROUP1.length}}">
          {{#if INDEX.GROUP1.url}}<a href="{{INDEX.GROUP1.url}}">{{/if}}{{INDEX.GROUP1.label}}{{#if INDEX.GROUP1.url}}</a>{{/if}}
        </th>
        {{/if}}
        {{#if INDEX.GROUP2.length}}
        <th scope="col" colspan="{{INDEX.GROUP2.length}}">
          {{#if INDEX.GROUP2.url}}<a href="{{INDEX.GROUP2.url}}">{{/if}}{{INDEX.GROUP2.label}}{{#if INDEX.GROUP2.url}}</a>{{/if}}
        </th>
        {{/if}}
        {{#if INDEX.GROUP3.length}}
        <th scope="col" colspan="{{INDEX.GROUP3.length}}">
          {{#if INDEX.GROUP3.url}}<a href="{{INDEX.GROUP3.url}}">{{/if}}{{INDEX.GROUP3.label}}{{#if INDEX.GROUP3.url}}</a>{{/if}}
        </th>
        {{/if}}
        {{#if INDEX.NOTES}}
        <th scope="col" rowspan="2">
          {{#if INDEX.NOTES.url}}<a href="{{INDEX.NOTES.url}}">{{/if}}{{INDEX.NOTES.label}}{{#if INDEX.NOTES.url}}</a>{{/if}}
        </th>
        {{/if}}
      </tr>
      <tr>
        {{#if INDEX.L1}}
          <th scope="col" rowspan="2">
            {{#if INDEX.L1.url}}<a href="{{INDEX.L1.url}}">{{/if}}{{INDEX.L1.label}}{{#if INDEX.L1.url}}</a>{{/if}}
          </th>
        {{/if}}
        {{#if INDEX.L2}}
          <th scope="col" rowspan="2">
            {{#if INDEX.L2.url}}<a href="{{INDEX.L2.url}}">{{/if}}{{INDEX.L2.label}}{{#if INDEX.L2.url}}</a>{{/if}}
          </th>
        {{/if}}
        {{#if INDEX.L3}}
          <th scope="col" rowspan="2">
            {{#if INDEX.L3.url}}<a href="{{INDEX.L3.url}}">{{/if}}{{INDEX.L3.label}}{{#if INDEX.L3.url}}</a>{{/if}}
          </th>
        {{/if}}
        {{#if INDEX.L4}}
          <th scope="col" rowspan="2">
            {{#if INDEX.L4.url}}<a href="{{INDEX.L4.url}}">{{/if}}{{INDEX.L4.label}}{{#if INDEX.L4.url}}</a>{{/if}}
          </th>
        {{/if}}
