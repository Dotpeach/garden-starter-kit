{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "modules" : true,
      "impliedStrict": true
    },
    "sourceType" : "module"
  },
  "globals": {
  },
  "plugins": [
  ],
  "rules": {
    "comma-dangle": ["error", "always"],
    "indent": ["error", 2],
  }
}
